{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["name", "description", "imageUrl", "muscles", "equipments"],
  "properties": {
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": ["workout 222"]
    },
    "description": {
      "type": "string",
      "default": "",
      "title": "The description Schema",
      "examples": ["nothngh here"]
    },
    "imageUrl": {
      "type": "string",
      "default": "",
      "title": "The imageUrl Schema",
      "examples": [
        "https://d357mttm70bw7x.cloudfront.net/6d11cb6e-cc67-4510-a182-561140ed0dce.jpg"
      ]
    },
    "videoUrl": {
      "type": "string",
      "default": "",
      "title": "The videoUrl Schema",
      "examples": [""]
    },
    "muscles": {
      "type": "array",
      "default": [],
      "title": "The muscles Schema",
      "items": {
        "type": "string",
        "title": "A Schema",
        "examples": ["1", "2", "4"]
      },
      "examples": [["1", "2", "4"]]
    },
    "equipments": {
      "type": "array",
      "default": [],
      "title": "The equipments Schema",
      "items": {
        "type": "string",
        "title": "A Schema",
        "examples": ["1", "2"]
      },
      "examples": [["1", "2"]]
    }
  },
  "examples": [
    {
      "name": "workout 222",
      "description": "nothngh here",
      "imageUrl": "https://d357mttm70bw7x.cloudfront.net/6d11cb6e-cc67-4510-a182-561140ed0dce.jpg",
      "videoUrl": "",
      "muscles": ["1", "2", "4"],
      "equipments": ["1", "2"]
    }
  ]
}
