{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["name", "notes", "imgUrl", "level", "workouts", "tags"],
  "properties": {
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": ["workout 222"]
    },
    "notes": {
      "type": "string",
      "default": "",
      "title": "The notes Schema",
      "examples": ["none"]
    },
    "imgUrl": {
      "type": "string",
      "default": "",
      "title": "The imgUrl Schema",
      "examples": [
        "https://images.healthshots.com/healthshots/en/uploads/2021/12/18081913/bhujangasana.jpg"
      ]
    },
    "level": {
      "type": "string",
      "default": "",
      "title": "The level Schema",
      "examples": ["2"]
    },
    "workouts": {
      "type": "array",
      "default": [],
      "title": "The workouts Schema",
      "items": {
        "type": "string",
        "default": "",
        "title": "A Schema",
        "examples": ["54"]
      },
      "examples": [["54"]]
    },
    "tags": {
      "type": "array",
      "default": [],
      "title": "The tags Schema",
      "items": {
        "type": "string",
        "title": "A Schema",
        "examples": ["2", "4"]
      },
      "examples": [["2", "4"]]
    }
  },
  "examples": [
    {
      "name": "workout 222",
      "notes": "none",
      "imgUrl": "https://images.healthshots.com/healthshots/en/uploads/2021/12/18081913/bhujangasana.jpg",
      "level": "2",
      "workouts": ["54"],
      "tags": ["2", "4"]
    }
  ]
}
